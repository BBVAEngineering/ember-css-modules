{
  "name": "ember-css-modules",
  "version": "0.7.7",
  "description": "CSS Modules for ambitious applications",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "lint-node": "eslint index.js config lib tests-node",
    "test-node": "qunit tests-node/{acceptance,unit}/**/*-test.js",
    "install-all": "cd tests/dummy-sass-addon && yarn && cd ../dummy-less-addon && yarn",
    "test": "yarn run lint-node && yarn run install-all && ember try:each",
    "test-win": "yarn run install-all && ember test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/salsify/ember-css-modules.git"
  },
  "engines": {
    "node": ">= 4"
  },
  "author": "Dan Freeman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/salsify/ember-css-modules/issues"
  },
  "homepage": "https://github.com/salsify/ember-css-modules#readme",
  "devDependencies": {
    "console-ui": "^2.0.1",
    "ember-ajax": "^3.0.0",
    "ember-browserify": "^1.2.0",
    "ember-cli": "^2.16.2",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-dependency-checker": "^2.0.1",
    "ember-cli-eslint": "^4.2.2",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.7.0",
    "ember-cli-qunit": "^4.0.2",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.5.0",
    "ember-sinon": "^1.0.1",
    "ember-source": "^2.16.2",
    "eslint": "^4.10.0",
    "loader.js": "^4.6.0",
    "postcss-color-rebeccapurple": "^3.0.0",
    "qunitjs": "^2.4.1",
    "sinon": "^4.1.2"
  },
  "keywords": [
    "ember-addon",
    "css-modules"
  ],
  "dependencies": {
    "broccoli-concat": "^3.2.2",
    "broccoli-css-modules": "^0.6.2",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-postcss": "^3.5.0",
    "calculate-cache-key-for-tree": "^1.1.0",
    "debug": "^3.1.0",
    "ember-cli-babel": "^6.9.0",
    "ember-cli-version-checker": "^2.1.0",
    "ensure-posix-path": "^1.0.2",
    "hash-string": "^1.0.0",
    "lodash.merge": "^4.6.0",
    "postcss": "^6.0.14",
    "toposort": "^1.0.6"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-font-awesome"
    ],
    "before": [
      "ember-cli-babel",
      "ember-cli-less",
      "ember-cli-sass",
      "ember-cli-stylus",
      "ember-cli-postcss"
    ],
    "paths": [
      "tests/dummy-addon",
      "tests/dummy-sass-addon",
      "tests/dummy-less-addon"
    ]
  }
}
