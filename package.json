{
  "name": "ember-css-modules",
  "version": "0.7.0-beta.1",
  "description": "CSS Modules for ambitious applications",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "lint-node": "eslint index.js config lib",
    "install-all": "cd tests/dummy-sass-addon && yarn && cd ../dummy-less-addon && yarn",
    "test": "yarn run lint-node && yarn run install-all && ember try:each",
    "test-win": "yarn run install-all && ember test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/salsify/ember-css-modules.git"
  },
  "engines": {
    "node": ">= 4"
  },
  "author": "Dan Freeman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/salsify/ember-css-modules/issues"
  },
  "homepage": "https://github.com/salsify/ember-css-modules#readme",
  "devDependencies": {
    "ember-ajax": "^3.0.0",
    "ember-browserify": "^1.1.13",
    "ember-cli": "^2.13.2",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^3.1.0",
    "ember-cli-htmlbars": "^2.0.2",
    "ember-cli-htmlbars-inline-precompile": "^0.4.3",
    "ember-cli-inject-live-reload": "^1.6.1",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.1.0",
    "ember-sinon": "^0.7.0",
    "ember-source": "^2.13.3",
    "eslint": "^3.19.0",
    "loader.js": "^4.4.1"
  },
  "keywords": [
    "ember-addon",
    "css-modules"
  ],
  "dependencies": {
    "broccoli-concat": "^3.2.2",
    "broccoli-css-modules": "^0.6.1",
    "broccoli-funnel": "^1.2.0",
    "broccoli-merge-trees": "^2.0.0",
    "debug": "^2.6.8",
    "ember-cli-babel": "^6.4.1",
    "ember-cli-version-checker": "^2.0.0",
    "ember-getowner-polyfill": "^1.2.3",
    "ensure-posix-path": "^1.0.2",
    "hash-string": "^1.0.0",
    "postcss": "^6.0.1",
    "toposort": "^1.0.3"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-babel",
      "ember-cli-less",
      "ember-cli-sass",
      "ember-cli-stylus",
      "ember-cli-postcss"
    ],
    "paths": [
      "tests/dummy-addon",
      "tests/dummy-sass-addon",
      "tests/dummy-less-addon"
    ]
  }
}
